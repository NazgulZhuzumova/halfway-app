
<div class="venue-container">
    <div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="https://media-cdn.tripadvisor.com/media/photo-s/17/8b/e3/dd/soho-coffee-co.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://aquabarandgrill.co.uk/shopimages/sections/extras/restauranta-aqua-bar-grill-italian-greak-spanish.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://julietangus.com/wp-content/uploads/2017/05/AquaSpirit_4.jpg" class="d-block w-100" alt="...">
    </div>
     <div class="carousel-item">
      <img src="https://media.gq-magazine.co.uk/photos/6113b4e21b7ed1398dea9033/master/w_1909,h_1282,c_limit/191105_THH_0122_LR_RT.jpg" class="d-block w-100" alt="...">
    </div>
     <div class="carousel-item">
      <img src="https://static.designmynight.com/uploads/2017/08/ab_disrepute_low_res_14-optimised.jpg" class="d-block w-100" alt="...">
    </div>
     <div class="carousel-item">
      <img src="https://images.squaremeal.co.uk/cloud/article/8811/images/best-vegan-restaurants-soho-old-compton-brasserie_05072019014245.png?w=1000" class="d-block w-100" alt="...">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>



    <%# <div class="venue-image">
      <img src="https://media-cdn.tripadvisor.com/media/photo-s/17/8b/e3/dd/soho-coffee-co.jpg" alt="">
    </div> %>

    <div class="venue-infos">
        <div class="venue-header text-center">
          <h2><%= @venue.name %></h2>
          <h4>London</h4>
          <h2><%="⭐️" * @venue.rating %></h2>
          <!-- <i class="place-holder-one fas fa-id-card"></i> !-->
          <!-- <i class="place-holder-two fas fa-id-card"></i> !-->
          <%= image_tag "shop-open.png", :class => "shop-open" %>
          <p class="shop-open-display">Open</p>
          <hr>
        </div>
        <div class="venue-list">
            <ul>
             <li><i class="fas fa-map-marker-alt"></i></i>  <%= @venue.address %> </li>

             <li><i class="fas fa-coffee"></i></i>  <%= @venue.category %> </li>
             <li>4 <%="⭐️" * @venue.rating %></li>

             <li><i class="fas fa-clock"></i> Open 9:00am - 5:00pm</li>
             <li><i class="fas fa-id-card"></i> <%= @venue.website %></li>
              <li><i class="fas fa-id-card"></i><a href="https://maps.google.com/?q=<%= @venue.address%>">Open Google Maps</a></li>

            </ul>
            <div class="d-flex justify-content-around">
              <%= link_to 'Back to your search', :back, class: "btn-flat" %>
            </div>
        </div>
    </div>
</div>
